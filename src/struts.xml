<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- Strutsの詳細設定 -->
<struts>
	<constant name="struts.devMode" value="true"/>
	<package name= "com.internousdev.ecsite.action" extends="struts-default">
		<!-- HomeAction -->
		<action name="HomeAction" class="com.internousdev.ecsite.action.HomeAction" method="execute">
			<result name = "login">login.jsp</result>
			<result name="success">buyItem.jsp</result>
		</action>
		<!-- LoginAction -->
		<action name="LoginAction" class="com.internousdev.ecsite.action.LoginAction" method="execute">
			<result name="success">buyItem.jsp</result>
			<result name="error">home.jsp</result>
		</action>
		<!-- GoHomeAction -->
		<action name="GoHomeAction" class="com.internousdev.ecsite.action.GoHomeAction" method="execute">
			<result name="success">home.jsp</result>
		</action>
		<!-- UserCreateAction-->
		<action name="UserCreateAction" class="com.internousdev.ecsite.action.UserCreateAction" method="execute">
			<result name="success">userCreate.jsp</result>
		</action>
		<!-- UserCreateConfirmAction -->
		<action name="UserCreateConfirmAction" class="com.internousdev.ecsite.action.UserCreateConfirmAction" method="execute">
			<result name="success">userCreateConfirm.jsp</result>
		</action>
		<!-- UserCreateCompleteAction -->
		<action name="UserCreateCompleteAction" class="com.internousdev.ecsite.action.UserCreateCompleteAction" method="execute">
			<result name="success">userCreateComplete.jsp</result>
		</action>
		<!-- BuyItemAction -->
		<action name="BuyItemAction" class="com.internousdev.ecsite.action.BuyItemAction" method="execute">
			<result name="success">buyItemConfirm.jsp</result>
		</action>
		<!-- BuyItemConfirmAction -->
		<action name="BuyItemConfirmAction" class="com.internousdev.ecsite.action.BuyItemConfirmAction" method="execute">
			<result name="success">buyItemComplete.jsp</result>
			<result name="error">outOfStock.jsp</result>
		</action>
		<!-- MyPageAction -->
		<action name="MyPageAction" class="com.internousdev.ecsite.action.MyPageAction" method="execute">
			<result name="success">myPage.jsp</result>
			<result name="error">myPage.jsp</result>
		</action>
		<!-- LogoutAction -->
		<action name="LogoutAction" class="com.internousdev.ecsite.action.LogoutAction" method="execute">
			<result name="success">home.jsp</result>
		</action>
		<!-- BusketAction -->
		<action name="BasketAction" class="com.internousdev.ecsite.action.BasketAction" method="execute">
			<result name="success">buyItem.jsp</result>
		</action>
		<!-- BusketEmptyAction -->
		<action name="BasketEmptyAction" class="com.internousdev.ecsite.action.BasketEmptyAction" method="execute">
			<result name="success">buyItemConfirm.jsp</result>
		</action>
		<!-- ManagerLoginAction -->
		<action name="ManagerLoginAction" class="com.internousdev.ecsite.action.ManagerLoginAction" method="execute">
			<result name="success">managerLogin.jsp</result>
		</action>
		<!-- ManagerAction -->
		<action name="ManagerAction" class="com.internousdev.ecsite.action.ManagerAction" method="execute">
			<result name="success">management.jsp</result>
			<result name="error">home.jsp</result>
		</action>
		<!-- StockManagementAction -->
		<action name="StockManagementAction" class="com.internousdev.ecsite.action.StockManagementAction" method="execute">
			<result name="success">stockManagement.jsp</result>
		</action>
		<!-- ReplenishAction -->
		<action name="ReplenishAction" class="com.internousdev.ecsite.action.ReplenishAction" method="execute">
			<result name="success">stockManagement.jsp</result>
		</action>
		<!-- ProductManagementAction -->
		<action name="ProductManagementAction" class="com.internousdev.ecsite.action.ProductManagementAction" method="execute">
			<result name="success">productManagement.jsp</result>
		</action>
		<!-- DeleteProductAction -->
		<action name="DeleteProductAction" class="com.internousdev.ecsite.action.DeleteProductAction" method="execute">
			<result name="success">productManagement.jsp</result>
		</action>
		<!-- AddProductAction -->
		<action name="AddProductAction" class="com.internousdev.ecsite.action.AddProductAction" method="execute">
			<result name="success">addProduct.jsp</result>
		</action>
		<!-- AddProductConfirmAction -->
		<action name="AddProductConfirmAction" class="com.internousdev.ecsite.action.AddProductConfirmAction" method="execute">
			<result name="success">addProductConfirm.jsp</result>
			<result name="error">addProduct.jsp</result>
		</action>
		<!-- AddProductCompleteAction -->
		<action name="AddProductCompleteAction" class="com.internousdev.ecsite.action.AddProductCompleteAction" method="execute">
			<result name="success">addProductComplete.jsp</result>
		</action>
		<!-- AdminAction -->
		<action name="AdminAction" class="com.internousdev.ecsite.action.AdminAction" method="execute">
			<result name="success">admin.jsp</result>
		</action>
		<!-- ItemCreateAction -->
		<action name="ItemCreateAction" class="com.internousdev.ecsite.action.ItemCreateAction" method="execute">
			<result name="success">itemCreate.jsp</result>
		</action>
		<!-- ItemCreateConfirmAction -->
		<action name="ItemCreateConfirmAction" class="com.internousdev.ecsite.action.ItemCreateConfirmAction" method="execute">
			<result name="success">itemCreateConfirm.jsp</result>
			<result name="error">itemCreate.jsp</result>
		</action>
		<!-- ItemCreateCompleteAction -->
		<action name="ItemCreateCompleteAction" class="com.internousdev.ecsite.action.ItemCreateCompleteAction" method="execute">
			<result name="success">itemCreateComplete.jsp</result>
		</action>
		<!-- ItemListAction -->
		<action name="ItemListAction" class="com.internousdev.ecsite.action.ItemListAction" method="execute">
			<result name="success">itemList.jsp</result>
		</action>
		<!-- UserListAction -->
		<action name="UserListAction" class="com.internousdev.ecsite.action.UserListAction" method="execute">
			<result name="success">userList.jsp</result>
		</action>
		<!-- ItemListDeleteConfirmAction -->
		<action name="ItemListDeleteConfirmAction" class="com.internousdev.ecsite.action.ItemListDeleteConfirmAction" method="execute">
			<result name="success">itemListDeleteConfirm.jsp</result>
		</action>
		<!-- ItemListDeleteCompleteAction -->
		<action name="ItemListDeleteCompleteAction" class="com.internousdev.ecsite.action.ItemListDeleteCompleteAction" method="execute">
			<result name="success">itemListDeleteComplete.jsp</result>
		</action>
		<!-- UserListDeleteConfirmAction -->
		<action name="UserListDeleteConfirmAction" class="com.internousdev.ecsite.action.UserListDeleteConfirmAction" method="execute">
			<result name="success">userListDeleteConfirm.jsp</result>
		</action>
		<!-- UserListDeleteCompleteAction -->
		<action name="UserListDeleteCompleteAction" class="com.internousdev.ecsite.action.UserListDeleteCompleteAction" method="execute">
			<result name="success">userListDeleteComplete.jsp</result>
		</action>
		<!-- UserManagementAction -->
		<action name="UserManagementAction" class="com.internousdev.ecsite.action.UserManagementAction" method="execute">
			<result name="success">userManagement.jsp</result>
		</action>
		<!-- AddUserAction -->
		<action name="AddUserAction" class="com.internousdev.ecsite.action.AddUserAction" method="execute">
			<result name="success">addUser.jsp</result>
		</action>
		<!-- AddUserConfirmAction -->
		<action name="AddUserConfirmAction" class="com.internousdev.ecsite.action.AddUserConfirmAction" method="execute">
			<result name="success">addUserConfirm.jsp</result>
			<result name="error">addUser.jsp</result>
		</action>
		<!-- AddUserCompleteAction -->
		<action name="AddUserCompleteAction" class="com.internousdev.ecsite.action.AddUserCompleteAction" method="execute">
			<result name="success">addUserComplete.jsp</result>
		</action>
		<!-- DeleteUserAction -->
		<action name="DeleteUserAction" class="com.internousdev.ecsite.action.DeleteUserAction" method="execute">
			<result name="success">userManagement.jsp</result>
		</action>
	</package>
</struts>